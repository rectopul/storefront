.process    
    &__container
        width: 100%
        display: flex
        flex-flow: column nowrap
        justify-content: flex-start
        align-content: flex-start
        position relative
    &__list
        flex 0 0 100%
        display: flex
        flex-flow: column nowrap
        justify-content: flex-start
        align-content: flex-start
        &_widget
            position relative
    &__item
        flex 0 0 45px
        width: 100%
        display: flex
        flex-flow: row nowrap
        justify-content: space-between
        align-items: center
        margin-bottom: 20px
        position relative
        box-shadow: 0 0 2px rgba(0,0,0,0.7)
        padding 5px 10px
        box-sizing: border-box
        border-radius: 3px
        &:hover
            & .process__coment
                display: block
        & article 
            flex 1
            padding 0 15px
        & svg 
            border-radius: 3px
            box-shadow: 1px 1px 3px rgba(0,0,0,0.1)
    &__form
        position: absolute
        width 100%
        height 100%
        left 50%
        bottom: 0
        display: flex        
        flex-flow: row nowrap
        justify-content: center
        align-items: flex-start
        max-height: 0;
        overflow hidden
        &.comentr
            max-height: 1000px
            overflow: initial
        & > div 
            position: relative
            display: flex
            flex-flow: column nowrap
            justify-content: flex-start
            align-items: center
            width: 100%
            left -50%
            display: block
            z-index: 3
            background-color: #FFFFFF
            box-shadow: 0 0 2px rgba(0,0,0,1)
            padding 10px
            border-radius: 4px
            & textarea 
                height 50px
                width 100%
            & p.form-submit
                .submit
                    background: #35C246
                    border-color: #114017 #22802D #22802D
                    box-shadow: inset 0 -1px 0 #22802D
                    color: #fff
                    text-decoration: none
                    text-shadow: 0 -1px 1px #114017, 1px 0 1px #114017, 0 1px 1px #114017, -1px 0 1px #114017
                    border-radius: 3px
                    display: block
                    padding 8px 12px !important
            & .logged-in-as,
            & .comment-reply-title
                display: none
                    
    &__coment
        position: absolute
        right: 5px
        top:  50%
        margin-top: -17px
        display: none
        &-button
            position: relative
            width: 100%
            height: 100%
            display: inline-block
            background: #35C246
            border-color: #114017 #22802D #22802D
            box-shadow: inset 0 -1px 0 #22802D
            color: #fff
            text-decoration: none
            text-shadow: 0 -1px 1px #114017, 1px 0 1px #114017, 0 1px 1px #114017, -1px 0 1px #114017
            border-radius: 3px
            padding 8px 10px
            transition: all 400ms
            z-index: 4
            &:hover
                background: #7EC289
                border-color: #2B422F #53805A #53805A
                box-shadow: inset 0 -1px 0 #53805A
                color: #fff
                text-decoration: none
                text-shadow: 0 -1px 1px #2B422F, 1px 0 1px #2B422F, 0 1px 1px #2B422F, -1px 0 1px #2B422F 
            &:focus
                outline: none 
