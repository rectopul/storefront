.button-ger-help
    height 120px
    display: flex
    flex-flow: row nowrap
    justify-content: center
    align-items: center
    & a 
        margin: 0
    & span 
        margin-right: 22px
        font-family: Roboto
        font-size: 18px
        font-weight: normal
        font-style: normal
        font-stretch: condensed
        line-height: 1.22
        letter-spacing: normal
        color: #8f8f8f
    &-container
        margin-top: 100px
.lamp-idea
    content url('data:image/svg+xml;base64,')
    width: 23px
    height 23px
    display: inline-block
    margin-right: 7.5px
.the-title
    display: inline-block
    font-family: Roboto
    font-size: 27px
    font-weight: normal
    font-style: normal
    font-stretch: condensed
    line-height: normal
    letter-spacing: normal
    text-align: center
    color: #252525
    margin: 0
.help
    &__min
        &-container
            display: flex
            flex-flow: row wrap
            justify-content: flex-start
        &-list
            flex: 0 0 33.333333333%
            display: flex
            flex-flow: column nowrap
            justify-content: flex-start
            align-items: flex-start
            font-family: Roboto
            font-size: 18px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            line-height: 2.5
            letter-spacing: normal
            color: #8f8f8f
            &:not(:nth-child(3n+0))
                padding-right: 120px
            & a 
                color: #8f8f8f
                &:hover
                    color #000000
            &  > li 
                line-height: 1
                margin-bottom: 30px
        &-title
            margin-top: 100px
            font-family: Roboto
            color: #252525
            line-height: normal
            letter-spacing: normal
            font-style: normal
            margin-bottom: 40px
            display: flex
            flex-flow: row nowrap
            justify-content: flex-start
            align-items: center
            & h3 
                display: inline-block
                margin: 0
            &:before
                content '' 
                background-image url('data:image/svg+xml;base64,')
                background-size: 23px
                background-repeat: no-repeat
                width: 23px
                height 23px
                display: inline-block
                vertical-align: middle
                margin-right: 7px
                margin-bottom: 4px
            & h3 
                font-size: 27px
                font-weight: normal                
                font-stretch: condensed                
        &-stick
            border-radius: 35px
            box-shadow: 2px 2px 6px 0 rgba(0, 0, 0, 0.1)
            background-color: #ffffff
            font-family: Roboto
            font-size: 18px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            line-height: 1.22
            letter-spacing: normal
            color: #8f8f8f
            padding 24.5px 35px
            transition: all 300ms
            &:hover
                background-color: #CCC
            & a
                display: block
                color: #8f8f8f
                &:hover
                    color: #FFFFFF
                    text-decoration: none
        &-sticktitle
            display: flex
            flex-flow: row nowrap
            justify-content: flex-start
            align-items: center
            margin-bottom: 45px
.archive__help
    display: flex
    flex-flow: column nowrap
    justify-content: center
    align-items: center
    height 100%
    position relative
    z-index: 3
    &-separator
            position absolute
            display: block
            height 90%
            top 50%
            left: 50%
            & i 
                position: relative
                width 0
                height 100%
                left -50%
                top -50%
                display: inline-block
                border-left: 1px solid #3f3f3f
    &-posts
        position relative  
        & .helppost-next
            background-image: url('data:image/svg+xml;base64,')
        & .helppost-prev
            background-image url('data:image/svg+xml;base64,')
        &-container
            overflow hidden
        &-title
            margin-bottom: 100px
            & , & h1 
                font-family: Roboto
                font-size: 35px
                font-weight: normal
                font-style: normal
                font-stretch: condensed
                line-height: normal
                letter-spacing: normal
                text-align: center
                color: #ffffff

        &-item 
            height calc(100% - 32px)
            display: flex
            flex-flow: column nowrap
            justify-content: space-between
            align-items: center
            padding: 60px 0
    &-catmenu 
        margin-top: 0
    & > .row
        width: 100%
    &-title
        & , & h1 
            font-family: Roboto
            font-size: 47px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            line-height: normal
            letter-spacing: normal
            text-align: center
            color: #ffde14
    &-form
        width: 100%
        flex: 0 0 76px
        height 76px
        background #ffffff
        display: flex
        flex-flow: column
        justify-content: center
        align-items: center
        border-radius: 140px
        width 780px
        max-width: 780px
        margin-top: 25px
        & > form 
            flex 0 0 100%
            width: 100%
            height 100%
            padding: 0 30px
            display: flex
            flex-flow: row-reverse nowrap
            align-items: center
            justify-content: space-between
            & label
                flex: 1
                height 100%
                margin: 0
                padding: 0
                & input 
                    width: 100%
                    border 0
                    height 100%
            & .search-submit 
                margin-right: 23px
                flex 0 0 23px
                height 23px
                background: transparent
                border 0
                outline: 0
                appearance none
                font-size: 0
                color: #8f8f8f
                background-image url('data:image/svg+xml;base64,')
                background-size: 100%
                background-position: center
                background-repeat: no-repeat
                &:hover
                    opacity 0.7
