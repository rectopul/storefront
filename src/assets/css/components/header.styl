

.pagecat
    &__block
        position: relative
        z-index: 3
        background-color: rgba(0, 0, 0, 0.5)
        padding-top: 70px
        padding-bottom: 70px
    &__message
        font-family: Roboto
        font-size: 18px
        font-weight: normal
        font-style: normal
        font-stretch: condensed
        line-height: 1.56
        letter-spacing: normal
        text-align: center
        color: #8f8f8f
        & a 
            & , &:hover
                color: #ffde14
                text-decoration: underline
        &-title
            font-family: Roboto
            font-size: 35px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            line-height: normal
            letter-spacing: normal
            text-align: center
            color: #ffffff
.linkyelow
    position relative   
    z-index: 3
    font-family: Roboto
    font-size: 22px
    font-weight: bold
    font-style: normal
    font-stretch: condensed
    line-height: normal
    letter-spacing: normal
    text-align: center
    color: #252525
    margin: 80px auto
    border-radius: 38px
    background-color: #ffde14
    padding 15px 50px
    text-decoration: none
    &:hover
        color #ffffff
        text-decoration: none
    &:visited,
    &:active
        color: #252525



.nav
    &__text
        position relative
        z-index: 3
        background #FFFFFF
        margin-top: 50px
        &-title
            text-align: left
            line-height: 50px
            font-family: Roboto
            font-size: 16px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            letter-spacing: normal
            color: #8f8f8f
        &-container
            font-family: Roboto
            font-size: 16px
            font-weight: normal
            font-style: normal
            font-stretch: condensed
            line-height: normal
            letter-spacing: normal
            text-align: center
            color: #252525
            & ul 
                display: flex
                flex-flow: row nowrap
                justify-content: flex-end
                align-items: center
                margin 0
                margin-left: -25px
                margin-right: -25px
                & li 
                    margin 0 25px
                    padding: 15px 0
                & a 
                    color: #252525
.headercat__messagestitle
    display: flex
    flex-flow: column nowrap
    justify-content: flex-start
    align-items: center
    position relative
    font-stretch: condensed
    font-family: Roboto
    font-weight: normal
    font-style: normal
    z-index: 3
    margin-top: 45px
    & h6
        font-size: 16px
        text-align: center
        color: #ffffff
        line-height: 1
    & h4 
        & , & > p
            font-size: 35px
            text-align: center
            color: #ffde14
            line-height: 50px
    & p 
        font-size: 20px
        font-weight: normal
        text-align: center
        color: #ffffff
header#header
    z-index 15
    height 405px
    position relative
    z-index 999
    background-repeat: no-repeat !important
    background-position: center top !important
    &.max-height
        height auto
        display: flex
        flex-flow: column nowrap
        justify-content: center
        background-repeat: no-repeat !important
    &:after
        content ''
        background RGBA(0, 0, 0, 0.70)
        position absolute
        width 100%
        height 100%
        top 0
        left 0
        display block
        z-index 0
.search-form
    position relative
    padding-top 105px
    z-index 3
    & h1 
        color #FFFFFF
.toast
    &.show
        z-index 20 !important

.cor01
    background: transparent
    color: cor04
    position fixed
    top 0
    left 0
    &.header-category
        top initial !important
        position relative
        background-image linear-gradient(45deg, #214858 0%, #467589 100%)
        margin-bottom 30px
.cor-down
    background-image linear-gradient(45deg, #214858 0%, #467589 100%)
.container_relative_menu
    width: 100%
    margin-left: -15px
    margin-right: -15px
    position relative
    height 65px
    display: flex
    flex-flow: row nowrap
    justify-content: center
.container__nav-menu
    width: 100%
    transition: all 300ms ease
    z-index: 999999999
.cont__navlogo
    position: relative
    display: block
    width: 1300px
    margin 0 auto
    height auto
    z-index: 20
.nav_logo
    padding 0 15px
    width: 100%
    flex-flow: row nowrap
    justify-content: space-between
    display: flex
    height 65px
    align-items: center
    position: relative
    z-index 5
    @media (max-width: 769px)
        justify-content: center

    &__customlogo
        flex: 0 0 40%
        display: flex
        flex-flow: row now
        justify-content: flex-start
        align-items: center
        margin 0
        height 100%
        @media screen and (max-width: 1024px)
            flex: 0 0 23%
        @media (max-width: 769px)
            flex: 0 0 70%
            justify-content: center

    &__menu-wraper
        flex: 1
        justify-content: flex-end
        display: flex
        align-items: center
        height 100%
        position static
        @media (max-width: 769px)
            display: none

    
    &__nav
        vertical-align middle
        align-self center
        display flex
        height 100%
        flex-flow row nowrap
        justify-content space-between
        align-items center
        margin 0
        font-size 13px
        font-weight bold
        font-style normal
        font-stretch condensed
        line-height normal
        letter-spacing normal
        text-transform uppercase
        color #FFFFFF
        letter-spacing 0
        position static
        & .Btn_Green
            & a
                background-image linear-gradient(225deg, #1EA4B0 0%, #52C498 100%)
                box-shadow 0 2px 12px 0 rgba(0,0,0,0.10)
                border-radius 25px
                text-shadow 0 1px 1px rgba(0,0,0,0.15)
                display inline-block
                padding 12px 23px

        & a 
            font-family Roboto
            font-size 13px
            font-weight 400
            font-style normal
            font-stretch condensed
            line-height normal
            letter-spacing normal
            color #FFFFFF
            text-decoration none

            &:hover
                text-decoration: none
                color: cor01
                + .opacity
                    display block

        & > li 
            flex: 0 0 auto
            position relative
            display flex
            flex-flow row nowrap
            justify-content center
            align-items center
            height 100%
            &[class*="whats"]
                & > a
                    display inline-block
                    line-height 14px
                    &:before
                        content url('data:image/svg+xml;base64,')
                        width 14px
                        height 15px
                        margin-right 7px
                        display inline-block
                        vertical-align middle
                        margin-top -2px
            &.menu-item-object-page
                & > a
                    color cor01
                    display inline-block
                    &:before
                        content url('data:image/svg+xml;base64,')
                        width 14px
                        height 15px
                        margin-right 7px
                        display inline-block
                        vertical-align middle
                        margin-top -2px
            & > a
                position relative
                &:after
                    content ''
                    width 100%
                    height 3px
                    display inline-block
                    position absolute
                    left 0
                    bottom -20px
                    background #FFE600
                    overflow hidden
                    max-width 0
                    transition max-width 400ms cubic-bezier(.23,.62,.51,.86) 0ms
                &:hover
                    &:after
                        max-width 400px
                        transition max-width 2000ms cubic-bezier(.23,.62,.51,.86) 0ms
            &.menu-item-has-children
                &:not(.sub-menu-lvl)
                    position relative
                    &:after
                        content ''
                        width 0
                        height 0
                        border-left 2.5px solid transparent
                        border-right 2.5px solid transparent
                        border-top 5.2px solid #FFFFFF
                        background-color: transparent
                        display inline-block
                        margin-left 4px
                    & > ul
                        width auto
                        left 50%
                        flex-flow column nowrap
                        padding 30px
                        white-space nowrap
                        & > li 
                            display flex
                            flex-flow column nowrap
                            margin 0
                            margin-bottom 10px
                            & a 
                                font-family Roboto
                                font-weight 400
                                font-size 13px
                                color #8F8F8F
                                letter-spacing 0
                                line-height 11px
                                &:hover
                                    color cor01
            &.sub-menu-lvl
                position static
                &:after
                    content ''
                    width 0
                    height 0
                    border-left 2.5px solid transparent
                    border-right 2.5px solid transparent
                    border-top 5.2px solid #FFFFFF
                    background-color: transparent
                    display inline-block
                    margin-left 4px                
            &:not(:first-child)
                margin-left: 27px
                &:before
                    content ''
                    width 0
                    height 14px
                    border-left solid 1px #4a4a4a
                    margin-right 27px
                    top 50%
                    margin-top -2px
            &:hover
                & > ul
                    display flex
            & > ul 
                position absolute
                display none
                flex-flow row nowrap
                justify-content center
                width 100%
                background #FFFFFF  
                left 0
                top 60px
                margin 0
                padding 40px 60px
                border 1px solid #F1F0F5
                box-shadow 9px 9px 10px 0 rgba(0,0,0,0.06)
                border-radius 0 0 43px 43px
                text-transform capitalize
                & > li
                    flex 0 0 auto
                    margin 0 50px
                    display inline-block
                    &[class*="viagem"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                display inline-block
                                margin-right 7px
                                width 18px
                                height 18px
                                vertical-align middle
                    &[class*="saude"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                width 18px
                                height 15.4px
                                margin-right 7px
                                display inline-block
                                vertical-align middle
                    &[class*="compras"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                width 17px
                                height 16px
                                margin-right 7px
                                display inline-block
                                vertical-align middle
                    &[class*="telefonia"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                width 12px
                                height 19px
                                margin-right 7px
                                display inline-block
                                vertical-align middle
                    &[class*="construct"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                width 11.2px
                                height 18px
                                margin-right 7px
                                display inline-block
                                vertical-align middle
                    &[class*="etc"]
                        & > a
                            &:before
                                content url('data:image/svg+xml;base64,')
                                width 15px
                                height 15px
                                margin-right 7px
                                display inline-block
                                vertical-align middle
                    & > a 
                        font-size 15px
                        color #252525
                        letter-spacing 0
                        font-family Roboto
                        font-weight 700
                        font-size 15px
                        letter-spacing 0
                    & > ul 
                        margin-left 0
                        position relative
                        display flex
                        flex-flow column nowrap
                        justify-content flex-start
                        margin-top 12px
                        & > li 
                            margin-bottom 14px
                        & a
                            font-family Roboto 
                            font-weight 300
                            font-size 13px
                            color #8F8F8F
                            letter-spacing 0
                            line-height 11px
    & .nav_mobile-menu
        width: calc(100vw - 30px)
        display: flex
        position: fixed
        flex-flow: row nowrap
        justify-content: flex-start
        z-index: 9
        padding: 0
        align-self: flex-start
        transform: translateX(calc(100vw - 15px))
        transition: all 0.4s

        & ul 
            display: flex
            flex: 0 0 100%
            overflow hidden
            height 100vh
            position relative
            flex-flow: column nowrap
            background-color: rgba(255, 255, 255, .99);  
            backdrop-filter: blur(5px);
            justify-content: flex-start
            align-items: flex-start
            background: transparent     
            color: #1a1d1c
            padding: 15px

            &::before 
                content: ''
                width: 100%
                height 100%
                position: absolute
                background: #FFFFFF
                top: 0px
                left: 0px
                opacity .94
                z-index: -1


            & > li 
                margin: 0  
                z-index: 1   

                &:not(:first-child)
                    margin-top: 20px           

            & a 
                text-shadow: none
                color: #0d0f0f

        & .corners
            display: flex
            flex: 0 0 40px
            width: 40px
            background: #1a1d1c
            padding: 12px
            height: 38px
            flex-flow: column nowrap
            justify-content: space-between
            align-items: center
            margin-right: 20px
            position: absolute
            left: -40px
            top: 15px

            & span
                flex: 0 0 2px
                background: #FFFFFF
                width: 100%
                border: 0


        @media (min-width: 769px)
            display: none
