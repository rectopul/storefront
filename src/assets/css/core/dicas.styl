.dicas
    z-index 0
    &__item
        background: #FFFFFF
        border: 1px solid #F1F0F5
        box-shadow: 9px 9px 10px 0 rgba(0,0,0,0.06)
        border-radius: 38px
        padding 20px 15px
        height 300px
        display flex
        flex-flow column nowrap
        justify-content center
        align-items center
        text-align center
        font-family: Roboto
        font-weight 300
        font-size: 16px
        color: #8F8F8F
        letter-spacing: -0.2px
        margin-bottom 30px
        & figure 
            margin-bottom 30px
        @media screen and (min-width: 993px)
            margin-bottom 0
            height 354px
            padding 35px 30px
    &__title
        & h1
            color: #252525
            font-weight 400
        & small 
            color: #8F8F8F
            font-weight 400

.processo
    &__title
        font-weight 400
        color: #FFDE14
    &__item
        display flex
        flex-flow column nowrap
        justify-content flex-start
        align-items center
        & article
            font-family: Roboto
            font-weight 300
            font-size: 16px
            color: #FFFFFF
            letter-spacing: 0
            line-height 20px
            padding-left 60px
            margin-top -25px
            z-index: 1
    &__col
        flex 0 0 240px
        & .count
            width 100%
            height 144px
            background transparent url('data:image/svg+xml;base64,') no-repeat left center
            background-size 187px 144px
            color #252525
            display flex
            flex-flow row nowrap
            justify-content center
            align-items center
            position relative
            margin-top 15px
            z-index: 0
            & span
                display inline-block
                width: auto
                height auto
                font-size: 60px
                font-weight 700
                font-style italic
                position: absolute
                left calc(50% - 20px)
                top calc(50% - 73px)
                & > i 
                    position relative
                    width: 100%
                    height: 100%
                    display inline-block
                    left -50%
                    top -50%
/* Dicas da empresa */ 
.numpartner
    margin-top: 60px
    & div[class*='numpartner__icon-']
        display: inline-block
        position: absolute
        left 10px
        top: 50%
        transition left 300ms ease
    &__icon
        &-2        
            content: url('data:image/svg+xml;base64,')
            width: 24px
            height 25px
            margin-top: -12px
        &-1
            content: url('data:image/svg+xml;base64,')
            width: 25px
            height 26px
            margin-top: -13px
        &-3
            content url('data:image/svg+xml;base64,')
            width 27px
            height 25px
            margin-top: -12.5px
    &__item
        display flex
        flex-flow row nowrap
        justify-content: space-between
        align-items: center
        position relative
        overflow hidden
        background transparent url('../../img/circle.png') no-repeat 
        background-size: 120px 120px;
        background-position: 158px 12px;
        &:after
            content: ''
            width: 3px
            background: #fff
            height: 211%
            position: absolute
            top: 0
            left: 50%
            margin-left: -208.5px
            margin-top: -143px
            transform: rotate(45deg)
            border-right: 206px solid #FFF
        &:before
            z-index: 0
            content: ''
            width: 1px
            background: transparent
            height: 75%
            border-radius: 39px
            position: absolute
            box-shadow: 1px 0 25px 10px rgba(0,0,0,0.4)
            top: 6%
            margin-top: -0.5px
            left: 48%
            transform: rotate(45deg)
            margin-left: -0.5px
        & > div
            flex 0 0 50%
            text-align: left
            font-family: Roboto
            font-weight: 400
            font-size: 16px
            color: #8F8F8F
            letter-spacing: 0
            display: flex
            flex-flow: row nowrap
            z-index: 3
            &:last-child                              
                height 177px
                justify-content: flex-start
                align-items: flex-end 
                position relative
                z-index: 0
                &:hover
                    &:before
                        left -40px
                    & div 
                        left 60px
                &:before
                    content ''
                    width: 162px
                    height 59px
                    background url('data:image/svg+xml;base64,') no-repeat center center  
                    position: absolute
                    top 50%
                    left -80px
                    margin-top: -30px
                    z-index: -1 
                    transition left 300ms ease
            &:first-child
                text-align right 
                font-family: Roboto 
                font-weight: 400
                font-size: 45px
                color: #252525
                letter-spacing: 0
                text-align: center
                line-height: 35px
                justify-content: flex-end
                align-items: center
                padding-right: 30px